{
  "name": "nativescript-svg",
  "version": "1.3.4",
  "description": "NativeScript SVG plugin",
  "main": "./build/svg",
  "typings": "./src/svg.d.ts",
  "nativescript": {
    "platforms": {
      "android": "3.0.0",
      "ios": "3.0.0"
    }
  },
  "scripts": {
    "build": "tsc",
    "demo.ios": "npm run preparedemo ; cd demo ; tns emulate ios",
    "demo.android": "npm run preparedemo ; cd demo ; tns run android",
    "preparedemo":
      "npm run build ; cd demo ; tns plugin remove nativescript-svg ; tns plugin add .. ; tns install",
    "setup":
      "cd demo ; npm install ; cd .. ; npm run build ; cd demo ; tns plugin add .. ; cd .."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/peoplewareDo/nativescript-svg.git"
  },
  "keywords": ["NativeScript", "JavaScript", "Android", "iOS", "svg"],
  "author": {
    "name": "Victor Sosa",
    "email": "victor.sosa@peopleware.do"
  },
  "contributors": [
    {
      "name": "Brad Martin",
      "email": "bmartin@nstudio.io",
      "url": "https://github.com/bradmartin"
    }
  ],
  "bugs": {
    "url": "https://github.com/peoplewareDo/nativescript-svg/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/peoplewareDo/nativescript-svg",
  "readmeFilename": "README.md",
  "devDependencies": {
    "tns-core-modules": "^3.0.0 || ^3.0.0-rc.1",
    "tns-platform-declarations": "^3.0.0 || ^3.0.0-rc.1",
    "typescript": "^2.2.2",
    "nativescript-angular": "^3.0.0 || ^2.0.0-rc.1",
    "@angular/core": "~4.0.1",
    "@angular/common": "~4.0.1",
    "@angular/compiler": "~4.0.1",
    "@angular/compiler-cli": "~4.0.1",
    "rxjs": "~5.3.0",
    "zone.js": "~0.8.4"
  }
}
